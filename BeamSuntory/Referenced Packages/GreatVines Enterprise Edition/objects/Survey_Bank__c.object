        <fullName>Name_With_Symbol__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>Question_Text__c +
CASE(
    Type__c,
    &apos;Number&apos;, $Label.gvp__survey_target_number,
    &apos;YesNo&apos;, $Label.gvp__survey_target_yesno,
    &apos;Currency&apos;, $Label.gvp__survey_target_dollar,
    &apos;Check&apos;, $Label.gvp__survey_target_check,
    &apos;List&apos;, $Label.gvp__survey_target_list,
    &apos;Radio&apos;, $Label.gvp__survey_target_radio,
    &apos;Percent&apos;, $Label.gvp__survey_target_percentage,
    &apos;Text&apos;, &apos;&apos;,
    &apos;Lookup&apos;, $Label.gvp__survey_target_reference,
    $Label.gvp__unknown_type
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Name With Symbol</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
